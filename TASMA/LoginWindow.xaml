<Window x:Class="TASMA.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TASMA"
        mc:Ignorable="d"
        Height="350" Width="525" WindowStyle="None" AllowsTransparency="True">
    <Grid>

        <Button Name="LoginWindow_Exit" Content="Exit" Width="40" Height="25" Background="Transparent" Margin="468,10,10.4,309" BorderThickness="0" Click="ExitRoutine"/>
        <Button Name="LoginWindow_LoginTest" Content="LoginTest" Width="80" Height="25" Background="Snow" Click="LoginTest"/>
    </Grid>
</Window>
